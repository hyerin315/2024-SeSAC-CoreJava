		int i = 65; // initialization 
		int a = 1;
		for ( ; i <= 90; ) {// 조건 나머지는 다 밖으로 빼도 됨
			System.out.printf("%c\t", (char)i);
			i++;
			a++;
			if (a % 5 == 0) {
				System.out.println();
			} else {
				System.out.println("\n");
			}
		}
		System.out.println();

	}

