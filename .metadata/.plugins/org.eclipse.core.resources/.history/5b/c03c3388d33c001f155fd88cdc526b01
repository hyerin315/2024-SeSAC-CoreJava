
public class DoDemo {

	public static void main(String[] args) {
		// 4! = 4 * 3 * 2 * 1
		java.util.Scanner sc = new java.util.Scanner(System.in);
		System.out.print("ëª‡ Factorial? : ");
		
		int su = sc.nextInt();
		int answer = 1;
		for (int i = su ; i >= 0 ; i--) {
			answer *= su; 
		}
	}

}
