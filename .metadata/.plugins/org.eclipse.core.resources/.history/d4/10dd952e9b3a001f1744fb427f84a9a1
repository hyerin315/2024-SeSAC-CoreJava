		int i = 90; // initialization 
		for ( ; i >= 65; ) {// 조건 나머지는 다 밖으로 빼도 됨
			System.out.printf("%c\t", (char)i);
			i -= 3; // i = i - 3
		}
		System.out.println();

	}

}
